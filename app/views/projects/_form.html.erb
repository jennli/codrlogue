<%# :title,:description,:project_link,:github_link %>

<%= simple_form_for @project, remote: true do |f| %>

<div class="form-group">
  <%= f.input :title %>
</div>
<div class="form-group">
  <%= f.input :description %>
</div>
<div class="form-group">
  <%= f.input :project_link %>
</div>
<div class="form-group">
  <%= f.input :github_link %>
</div>
<div class="form-group">
  <%= f.submit "Add New Project", class: "btn btn-primary btn-sm" %>
</div>
<% end %>
